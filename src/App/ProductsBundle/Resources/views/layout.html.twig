        <link rel="icon" sizes="16x16" href="{{ asset('/favicon.ico') }}" />

    {% endblock %}
</head>
<body>
    {% block body %}
        <div id="content">
            <div class="sf-reset">
                <div class="block">
                    {% block content %}
